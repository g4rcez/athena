{
	"name": "js-validations",
	"version": "1.0.0",
	"description": "A little collection with data validation, in Javascript...of course",
	"homepage": "https://github.com/vandalvnl/js-validations#readme",
	"main": "./dist/index.js",
	"scripts": {
		"watch": "babel -w src/ -d dist/",
		"build": "babel src/ -d dist/",
		"start": "babel -w src/ -d dist/ | nodemon --watch dist",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"contributors": [
		{
			"jeanstartx": "https://github.com/jeanstartx"
		}
	],
		"repository": {
		"type": "git",
		"url": "https://github.com/vandalvnl/js-validations.git"
	},
	"keywords": [
		"schema",
		"validation",
		"objects",
		"regex",
		"filter",
		"filters",
		""
	],
	"author": "vandalvnl <vandal.vnl.dev@gmail.com> (https://github.com/vandalvnl)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/vandalvnl/js-validations/issues"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		],
		"plugins": [
			"transform-object-rest-spread",
			"transform-class-properties"
		]
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"plugins": [
			"import",
			"jest"
		],
		"parserOptions": {
			"ecmaVersion": 2017,
			"sourceType": "module"
		},
		"env": {
			"node": true,
			"jest": true
		},
		"extends": [
			"eslint:recommended"
		],
		"rules": {
			"jest/no-focused-tests": 2,
			"jest/no-identical-title": 2
		}
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"moment": "^2.22.2",
		"ramda": "^0.25.0"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^8.2.6",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"nodemon": "^1.18.3"
	},
	"keywords": [],
	"author": "",
	"license": "ISC"
}
